INI,__LIB_ST7565_SPI_HRES
INI,__LIB_ST7565_SPI_VRES
INI,__LIB_ST7565_SPI_INIT
INI,__LIB_ST7565_SPI_INIT_LEN

; set ST7565 display initializaton data (INIT)
ASM
MOV (__LIB_ST7565_SPI_HRES), ({VALUE})
MOV (__LIB_ST7565_SPI_VRES), ({VALUE} + 1)
MOV (__LIB_ST7565_SPI_INIT_LEN), ({VALUE} + 2)
LDW X, {VALUE} + 3
LDW (__LIB_ST7565_SPI_INIT), X
ENDASM