IMP,__LIB_ST7565_SPI_DC_LO

; set contrast (CONTRAST)
; A - contrast value (1..100)
:__LIB_ST7565_SPI_15_CALL
ASM
PUSH A
ENDASM

CALL,__LIB_ST7565_SPI_DC_LO

; set EV level command
PUT,SPI,0x81<BYTE>

ASM
LD A, 63
LD XL, A
POP A
MUL X, A
LD A, 100
DIV X, A
LD A, XL
ENDASM

; the command data byte
ASM
.IF SUBSTR(__MCU_NAME,0,5) == STM8S
ENDASM
INL,__LIB_SPI_PUT_B_INL
ASM
.ELSE
ENDASM
INL,__LIB_SPI1_PUT_B_INL
ASM
.ENDIF
ENDASM

RET