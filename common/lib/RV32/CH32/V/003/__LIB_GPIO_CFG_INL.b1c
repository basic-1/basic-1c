ASM
.IF {MASK} == 0xFFFFFFFF
LI A0, {VALUE}
SW A0, GPIO{DEV_NAME,1,1}_CFGLR
LI A0, {VALUE0}
SH A0, GPIO{DEV_NAME,1,1}_OUTDR
.ELSE
LW A0, GPIO{DEV_NAME,1,1}_CFGLR
ANDI A0, A0, !{MASK}
.IF {VALUE} != 0
ORI A0, A0, {VALUE}
.ENDIF
SW A0, GPIO{DEV_NAME,1,1}_CFGLR
LHU A0, GPIO{DEV_NAME,1,1}_OUTDR
ANDI A0, A0, !{MASK0}
.IF {VALUE0} != 0
ORI A0, A0, {VALUE0}
.ENDIF
SH A0, GPIO{DEV_NAME,1,1}_OUTDR
.ENDIF
ENDASM