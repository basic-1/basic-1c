10 REM write a string to UART (8N1 57600 baud)
20 IOCTL UART, PINS, DEFAULT
30 IOCTL UART, SPEED, 57600
40 IOCTL UART, ENABLE
50 IOCTL UART, START
60 PRINT "Hello world at 57600 baud!"
70 GOSUB 1000
80 GOTO 60
90 END

1000 REM delay subroutine
1010 FOR L1 = 0 TO 9
1020 FOR L2 = -32768 TO 32767
1030 NEXT L2
1040 NEXT L1
1050 RETURN